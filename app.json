{
  "expo": {
    "name": "AntDExpoApp",
    "slug": "AntDExpoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splashlogo.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashlogo.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashlogo.svg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/splashlogo.svg"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
